#Right-Arm {
  position: absolute;
  top: 153px;
  right: 557px;
  -webkit-transform: rotate(-8deg);
  opacity: .7;
  border-radius: 50px;
}

#Right-Arm:hover {
  opacity: 1;
  cursor: pointer;
}

#Left-Arm {
  height: 400px;
  width: 60px;
  position: absolute;
  top: 152px;
  left: 525px;
  -webkit-transform: rotate(9deg);
  opacity: .7;
  border-radius: 50px;
}

#Left-Arm:hover {
  cursor: pointer;
  opacity: 1;
}

